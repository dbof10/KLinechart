{
  "name": "klinecharts",
  "version": "9.8.8",
  "description": "Enhanced k-line chart built with html5 canvas",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/umd/klinecharts.min.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "LICENSE",
    "README.md",
    "dist/",
    "licenses/",
    "NOTICE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dbof10/KLinechart"
  },
  "keywords": [
    "k-line",
    "time-line",
    "candlestick",
    "indicator",
    "finance",
    "klinechart",
    "stockchart",
    "stock",
    "chart",
    "canvas"
  ],
  "author": "ti bull",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "node scripts/clean.js",
    "build-esm": "node scripts/build-esm.js",
    "build-cjs": "cross-env NODE_ENV=production node scripts/build-cjs.js",
    "build-umd:dev": "cross-env NODE_ENV=development node scripts/build-umd.js",
    "build-umd:prod": "cross-env NODE_ENV=production node scripts/build-umd.js",
    "build-umd": "npm run build-umd:dev && npm run build-umd:prod",
    "build-core": "npm run build-esm && npm run build-cjs && npm run build-umd",
    "build": "npm run clean && npm run build-core",
    "prepare": "husky install",
    "docs:dev": "npm run dev --prefix docs",
    "docs:build": "npm run build --prefix docs",
    "docs:preview": "npm run preview --prefix docs",
    "docs:deploy": "npm run docs:build && gh-pages -d website"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "devDependencies": {
    "@babel/standalone": "^7.24.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@shikijs/vitepress-twoslash": "^1.4.0",
    "@stackblitz/sdk": "^1.9.0",
    "@types/node": "^20.12.7",
    "chalk": "^5.3.0",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.57.0",
    "eslint-config-love": "^47.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^4.16.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5",
    "vitepress": "^1.1.4"
  }
}
